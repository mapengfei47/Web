#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_color-support@1.1.3@color-support/bin.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_color-support@1.1.3@color-support/bin.js" "$@"
  ret=$?
fi
exit $ret
