#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_gulp-cli@2.2.0@gulp-cli/bin/gulp.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_gulp-cli@2.2.0@gulp-cli/bin/gulp.js" "$@"
  ret=$?
fi
exit $ret
